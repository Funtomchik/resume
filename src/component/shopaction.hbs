<section class="m-5">
    <div class="card text-center ps-3 pb-3 pt-3 text-bg-success">
        {{#with service}}
        <div class="text-capitalize">
            <h2>{{title}}</h2>
            <br>
            <p class="text-outline-light">{{description}}</p>
            <br>
            {{#each buttons}}
            <a href="{{link}}" class="btn btn-primary" class="btn btn-link">{{text}}</a>
            {{/each}}
        </div>
        {{/with}}
    </div>
</section>

{{#with subscribe}}
<div class="card">
    <div class="card-body text-bg-dark">
        <div class="col-12">
            <ul class="nav nav-pils">
                <li class="col-5">
                    <h2>{{header}}</h2>
                    <p class="mb-0">{{description}}</p>
                </li>
                {{#each buttons}}
                <li class="m-2 ms-4 mt-3 me-0">
                    <a class="btn btn-primary" href="{{link}}" role="button">{{text}}</a>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>
{{/with}}