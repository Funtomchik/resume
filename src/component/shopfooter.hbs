<section class="m-5">
    <div class="row">
        {{#each footer}}
        <div class="nav col-2">
            {{#each this}}
            <div>
                <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{this.link}}">{{this.text}}</a>
            </div>
            {{/each}}
        </div>
        {{/each}}
    </div>
</section>