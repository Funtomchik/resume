<div>
    <ul>
        {{#each list}}
        <li>
            <button {{#if @last }}disabled{{/if}}>{{name}} - {{@index}}</button>
        </li>
        {{/each}}

        {{#each user}}
        <li>
            {{@key}}:
            {{@index}}
            {{#if value}} {{value}} {{else}} {{text}} {{/if}},
        </li>
        {{/each}}
    </ul>
</div>